[ req ] 
default_bits = 1024 # Size of keys 
default_keyfile = ikey.pem # name of generated keys 
default_md = md5 # message digest algorithm 
string_mask = nombstr # permitted characters 
distinguished_name = req_distinguished_name 

[ req_distinguished_name ] 
# Variable name   Prompt string 
#----------------------   ---------------------------------- 
0.organizationName = Wojewódzki Szpital Zespolony im L.Perzyny w Kaliszu 
organizationalUnitName = Andrzej Marcinkowski 
emailAddress = andrzej.marcinkowski@gmail.com
emailAddress_max = 40 
localityName = Kalisz
stateOrProvinceName = WLKP 
countryName = PL
countryName_min = 2 
countryName_max = 2 
commonName = AM 
commonName_max = 64 
extendedKeyUsage = critical,timeStamping

# Default values for the above, for consistency and less typing. 
# Variable name   Value 
#------------------------------   ------------------------------ 
0.organizationName_default =  Wojewódzki Szpital Zespolony im L.Perzyny w Kalis
localityName_default = Kalisz 
stateOrProvinceName_default = Kalisz 
countryName_default = PL

[ v3_ca ] 
basicConstraints = CA:TRUE 
subjectKeyIdentifier = hash 
authorityKeyIdentifier = keyid:always,issuer:always 

